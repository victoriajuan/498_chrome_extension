{
    "manifest_version": 2,
    "name": "My Extension",
    "version": "1.0",
  
    "description": "A plain text description",
    "browser_action":{
        "default_icon": "icon.png",
        "default_popup": "popup.html",
        "default_title": "FOCUS ENFORCER TO THE RESCUE"
    },
    "permissions": [
        "tabs",
        "activeTab",
        "storage",
        "notifications",
        "sessions",
        "alarms"
    ],
    "web_accessible_resources": [
        "icon.png"
    ],
    "background":{
        "scripts":["eventPage.js"],
        "persistent":false
    }
  }