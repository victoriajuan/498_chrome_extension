{
    "manifest_version": 2,
    "name": "My Extension",
    "version": "1.0",
  
    "description": "A plain text description",
    "browser_action":{
        "default_icon": "icon.png",
        "default_popup": "popup.html",
        "default_title": "Change Color Here!"
    },
    "permissions": [
        "activeTab",
        "storage",
        "notifications"
    ],
    "web_accessible_resources": [
        "icon.png"
    ]
  }